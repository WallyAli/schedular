
  <fieldset class="nested-fields">

    <%= f.input_field :first_name, placeholder: 'Child First Name' %></br>
    <%= f.input_field :last_name, placeholder: 'Child Last Name' %></br>
    <%= f.input_field :gender, collection: [:Male, :Female], prompt: true %></br>
    <%= f.input_field :birthday, :as => :datepicker, placeholder: 'Child Birth Day' %></br>

    <%= f.association :schedules, input_html: {class: 'field'} %>

    <p><%= link_to_remove_association "remove child", f %></p>
</fieldset>

