
  <fieldset class="nested-fields">

    <%= f.input :first_name, label: false, placeholder: 'Child First Name' %>
    <%= f.input :last_name, label: false, placeholder: 'Child Last Name' %>
    <%= f.input :gender, label: false, collection: [:Male, :Female], prompt: true %>
    <%= f.input :birthday, label: false, input_html: {class: 'datepicker'}, placeholder: 'Date of Birth' %>

    <%= f.association :schedules, input_html: {class: 'field'} %>

    <p><%= link_to_remove_association "remove child", f %></p>
</fieldset>

  