<%= simple_form_for([@child, @attendance]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :time_in %>
    <%= f.input :time_out %>
    <%= f.input :date %>
    <%= f.input :time_zone, priority: /US/ %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

